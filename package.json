{
  "name": "v-page",
  "description": "A simple pagination bar",
  "version": "2.1.0",
  "author": "TerryZ <terry5@foxmail.com>",
  "main": "dist/v-page.js",
  "files": [
    "/dist",
    "/types"
  ],
  "typings": "types/index.d.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test:unit": "mocha-webpack --webpack-config webpack.config.js --require tests/unit/setup.js tests/unit/**/*.spec.js",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test:unit",
    "vite": "vite",
    "build:vite": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryZ/v-page.git"
  },
  "keywords": [
    "front-end",
    "web",
    "vue",
    "vuejs",
    "page",
    "pagination"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "bootstrap": "^5.2.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.1"
  },
  "nyc": {
    "include": [
      "src/Page.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
