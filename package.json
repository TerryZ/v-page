{
  "name": "v-page",
  "description": "A simple pagination bar",
  "version": "3.0.0-beta.1",
  "author": "TerryZ <terry5@foxmail.com>",
  "type": "module",
  "files": [
    "/dist",
    "/types"
  ],
  "main": "./dist/v-page.umd.cjs",
  "module": "./dist/v-page.js",
  "exports": {
    ".": {
      "import": "./dist/v-page.js",
      "require": "./dist/v-page.umd.cjs"
    }
  },
  "typings": "types/index.d.ts",
  "license": "MIT",
  
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TerryZ/v-page.git"
  },
  "keywords": [
    "front-end",
    "web",
    "vue",
    "vuejs",
    "page",
    "pagination"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "^2.0.2",
    "bootstrap": "^5.2.0",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-css-injected-by-js": "^2.0.3",
    "vitest": "^0.21.0"
  }
}
